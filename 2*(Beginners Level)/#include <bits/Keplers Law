 #include <iostream>
 #include<cmath>

using namespace std;

int main()
{
    int t;
    cin>>t;
    while (t>0)
    {
        float t1,t2,r1,r2;
        cin>>t1>>t2>>r1>>r2;
        float a=(t1*t1)/(r1*r1*r1);
        float b=(t2*t2)/(r2*r2*r2);
        if (a==b)
        {
            cout<<"Yes"<<endl;
        }
        else
        {
            cout<<"No"<<endl;
        }
        t--;
    }
    return 0;
}
