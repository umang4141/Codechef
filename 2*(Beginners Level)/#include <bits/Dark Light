#include <bits/stdc++.h>
using namespace std;

#define ll long long int

int main()
{
    int t;
    cin>>t;
    
    int n, k;
    
    while(t--)
    {
        cin>>n>>k;
        
        if(k)
        {
            if(n%4==0)
                cout<<"On\n";
            else
                cout<<"Ambiguous\n";
        }
        else
        {
            if(n%4==0)
                cout<<"Off\n";
            else
                cout<<"On\n";
        }
    }
}
